{
   "action": {
      "default_icon": {
         "128": "/images/icon.png",
         "16": "/images/icon.png",
         "32": "/images/icon.png",
         "48": "/images/icon.png"
      },
      "default_popup": "popup.html"
   },
   "background": {
      "service_worker": "background.js",
      "type": "module"
   },
   "commands": {
      "salesAnalysis": {
         "description": "sales analysis",
         "suggested_key": {
            "default": "Ctrl+Shift+1",
            "mac": "Command+Shift+1"
         }
      },
      "secretOn": {
         "description": "secret On",
         "suggested_key": {
            "default": "Ctrl+Shift+2",
            "mac": "Command+Shift+2"
         }
      }
   },
   "content_scripts": [ {
      "js": [ "script.js" ],
      "match_origin_as_fallback": true,
      "matches": [ "*://*/*" ],
      "type": "module"
   } ],
   "declarative_net_request": {
      "rule_resources": [ {
         "enabled": true,
         "id": "ruleset_1",
         "path": "rules.json"
      } ]
   },
   "description": "Couplus Chrome Extension",
   "host_permissions": [ "*://*/*", "https://m.coupang.com/*", "https://promo.coupang.com/*", "https://wing.coupang.com/*", "https://supplier.coupang.com/*" ],
   "icons": {
      "128": "/images/icon.png",
      "16": "/images/icon.png",
      "32": "/images/icon.png",
      "48": "/images/icon.png"
   },
   "key": "",
   "manifest_version": 3,
   "name": "COUPLUS",
   "permissions": [ "activeTab", "scripting", "tabs", "cookies", "declarativeNetRequest", "alarms", "storage" ],
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "10.40"
}
